;\n    this.setState({ user: null });\n  };\n\n  handleSignupOrLogin = () => {\n    this.setState({ user: authService.getUser() });\n    this.props.history.push('/activity/heuristics');\n  };\n\n  render() {\n    const { user } = this.state;\n    return (\n      <>\n        <NavBar user={user} handleLogout={this.handleLogout} />\n\n        {/* <U.Main> */}\n\n\n        {/* write conditional routing to proper homepage depending on user type */}\n        {/* only get access to certain pages depending on user type */}\n        <Route\n          exact\n          path=\"/\"\n          render={() => (user ? <User user={user} /> : \n            <TempLanding />\n          // <Landing />\n            )}\n          />\n\n        {/* // Signup & Login Routes */}\n        <Route\n          \n          path=\"/signup/:groupId?/:email?\"\n          render={({ history, match }) => (\n            <Signup\n            history={history}\n            handleSignupOrLogin={this.handleSignupOrLogin}\n            match={match}\n            />\n            )}\n            />\n        <Route\n          exact\n          path=\"/login\"\n          render={({ history }) => (\n            <Login\n            history={history}\n            handleSignupOrLogin={this.handleSignupOrLogin}\n            />\n            )}\n            />\n\n        {/* // General Routes */}\n\n        <Route exact path=\"/activities\" render={() => <IndexActivities />} />\n        {/* <Route exact path=\"/activities/create\" render={() => <CreateActivityRefactor />} /> */}\n        <Route exact path=\"/manager-dashboard\" render={() => <Manager />} />\n        <Route\n          exact\n          path=\"/preview-activity\"\n          render={({ location }) => <PreviewActivity location={location} />} />\n        <Route \n          exact path=\"/corytestingground\" \n          render={() => <CoryTestingGround />} />\n        <Route\n          exact path=\"/passwordresetrequest\"\n          render={() => <PasswordResetRequest />} />\n        {/* // Heuristics Route */}\n        <Route\n          exact path=\"/activity/heuristics\"\n          render={({history, location}) => <HeuristicsActivity />} />\n        {/* </U.Main> */}\n        {/* // 10 Heuristics sub-routes for sidebar */}\n      </>\n    );\n  }\n}\n\nexport default App;\n","/Users/karen/Code/theCOOP/project-colab/src/services/authService.js",["210"],"/Users/karen/Code/theCOOP/project-colab/src/components/TuxComponents/UniversalComponents.js",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/Signup/Signup.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/Login/Login.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/Landing/Landing.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/User/User.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/CreateActivity/CreateActivity.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/CoryTestingGround/CoryTestingGround.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/HeuristicsActivity/HeuristicsActivity.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/PasswordResetRequest/PasswordResetRequest.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/IndexActivities/IndexActivities.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/PreviewActivity/PreviewActivity.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/TempLanding/TempLanding.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/pages/Manager/Manager.jsx",["211","212","213","214","215"],"/Users/karen/Code/theCOOP/project-colab/src/components/NavBar/NavBar.jsx",[],"/Users/karen/Code/theCOOP/project-colab/src/services/tokenService.js",["216"],"function setToken(token) {\n  localStorage.setItem(\"token\", token);\n}\n\nfunction getToken() {\n  let token = localStorage.getItem(\"token\");\n  if (token) {\n    // Check if expired, remove if it is\n    const payload = JSON.parse(atob(token.split(\".\")[1]));\n    // JWT's exp is expressed in seconds, not milliseconds, so convert\n    if (payload.exp < Date.now() / 1000) {\n      localStorage.removeItem(\"token\");\n      token = null;\n    }\n  }\n  return token;\n}\n\nfunction getUserFromToken() {\n  const token = getToken();\n  return token ? JSON.parse(atob(token.split(\".\")[1])).user : null;\n}\n\nfunction removeToken() {\n  localStorage.removeItem(\"token\");\n}\n\nexport default {\n  setToken,\n  getToken,\n  getUserFromToken,\n  removeToken,\n};","/Users/karen/Code/theCOOP/project-colab/src/services/userService.js",["217"],"import tokenService from \"../services/tokenService\";\nconst BASE_URL = \"/api/users/\";\n\n// restrict access to functions on the front end\n\n// this one works cory\nfunction getAllUsers() {\n  return fetch(\n    BASE_URL,\n    {\n      headers: { 'Authorization': \"Bearer \" + tokenService.getToken() },\n    },\n    { mode: \"cors\" }\n  ).then((res) => res.json());\n}\n\n// this one works cory\nfunction deleteUser(id) {\n  return fetch(`${BASE_URL}${id}`, {\n    method: 'DELETE',\n    headers: {\n      'Authorization': 'Bearer ' + tokenService.getToken()\n    }\n    }, \n    {mode: \"cors\"})\n  .then(res => res.json());\n}\n\n// this one works cory\nfunction getUser(user) {\n  return fetch(`${BASE_URL}${user._id}`, {mode: \"cors\"})\n  .then(res => res.json())\n}\n\n// this one works cory\nfunction updateUser(user) {\n  // console.log('update service function', user)\n  return fetch(`${BASE_URL}${user._id}`, {\n      method: \"PUT\",\n      headers: {\n        'content-type': 'application/json', \n        'Authorization': 'Bearer ' + tokenService.getToken()\n      },\n      body: JSON.stringify(user)\n  }, {mode: \"cors\"})\n  .then(res => res.json());\n}\n\nexport default {\n  getAllUsers,\n  deleteUser,\n  getUser,\n  updateUser\n}","/Users/karen/Code/theCOOP/project-colab/src/components/TuxComponents/LoginComponents.js",[],"/Users/karen/Code/theCOOP/project-colab/src/services/mail-api.js",[],"/Users/karen/Code/theCOOP/projec